ction&gt;&lt;/section&gt;&lt;/section&gt;&lt;/section&gt;&lt;section data-role=&quot;paragraph&quot; class=&quot;&quot; style=&quot;font-size: 16px;border-width: 0px;border-style: none;border-color: initial;padding: 0px;box-sizing: border-box;&quot;&gt;&lt;section style=&quot;padding: 2px 10px;box-sizing: border-box;&quot;&gt;&lt;section style=&quot;margin-right: 10px;width: 50px;height: 50px;border-width: 2px;border-style: solid;border-color: rgb(139, 162, 176);border-radius: 50%;float: left;box-sizing: border-box;&quot;&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;img border=&quot;0&quot; class=&quot;&quot; data-ratio=&quot;0.9392789373814042&quot; data-type=&quot;png&quot; data-w=&quot;527&quot; height=&quot;&quot; data-src=&quot;http://mmbiz.qpic.cn/mmbiz_jpg/GVQYSWTq9UrbO0iaHNR2CDJeficBxluQmNPXzScbQdZcicGhyJxm2H2UpkwnjNEYsGkCSAFuFxB3QaoFLLxRWmWdw/0&quot; style=&quot;border-radius: 50%; width: auto !important; height: auto !important; visibility: visible !important;&quot; title=&quot;undefined&quot; width=&quot;&quot; src=&quot;http://mmbiz.qpic.cn/mmbiz_jpg/GVQYSWTq9UrbO0iaHNR2CDJeficBxluQmNPXzScbQdZcicGhyJxm2H2UpkwnjNEYsGkCSAFuFxB3QaoFLLxRWmWdw/640?tp=webp&amp;amp;wxfrom=5&amp;amp;wx_lazy=1&quot; data-fail=&quot;0&quot;&gt;&lt;/p&gt;&lt;/section&gt;&lt;section class=&quot;&quot; data-style=&quot;clear: none;line-height:17px;padding:0 0;font-size:12px;&quot; style=&quot;margin: 5px;&quot;&gt;&lt;p style=&quot;clear: none;font-size: 12px;line-height: 1.5em;&quot;&gt;&lt;span style=&quot;color:#8ba2b0;&quot;&gt;&lt;strong&gt;李远&lt;/strong&gt;&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;clear: none;font-size: 12px;line-height: 1.5em;&quot;&gt;SelfSell 创始人&lt;/p&gt;&lt;/section&gt;&lt;/section&gt;&lt;section style=&quot;margin: 5px;border-width: initial;border-style: none;border-color: initial;box-sizing: border-box;&quot;&gt;&lt;section style=&quot;margin-left: 18px;width: 0px;border-bottom: 0.6em solid rgb(139, 162, 176);border-top-color: rgb(139, 162, 176);